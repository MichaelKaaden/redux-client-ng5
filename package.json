{
  "name": "redux-client-ng5",
  "version": "1.1.6",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-de": "ng build --configuration de",
    "build-en": "ng build --configuration en",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path src/locale --i18nFormat xlf --i18n-locale en && xliffmerge --profile xliffmerge.json en de",
    "serve-de": "ng serve --configuration de",
    "serve-en": "ng serve --configuration en",
    "postcommit": "git update-index --again",
    "precommit": "lint-staged",
    "prepush": "ng test --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/router": "^9.0.0",
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "7.2.10",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "7.2.10",
    "@angular/compiler": "7.2.10",
    "@angular/core": "7.2.10",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "7.2.10",
    "@angular/http": "7.2.10",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "7.2.10",
    "@angular/platform-browser-dynamic": "7.2.10",
    "@angular/router": "7.2.10",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "rxjs": "6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "7.2.10",
    "@angular/language-service": "7.2.10",
    "@types/jasmine": "^2.8.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "@types/redux-logger": "^3.0.7",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.5",
    "ngx-i18nsupport": "^0.16.2",
    "prettier": "^1.16.4",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.2.4"
  },
  "lint-staged": {
    "*.{scss,js,css}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --print-width 80 --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint -c tslint.json --fix -t stylish",
      "git add"
    ]
  }
}
